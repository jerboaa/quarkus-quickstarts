MAVEN_OPTS=-XX:+UseCompressedOops -XX:+UseCompressedClassPointers
MAVEN_ARGS=-Dquarkus.package.create-appcds=true package
S2I_SOURCE_DEPLOYMENTS_FILTER=quarkus-app
JAVA_OPTS_APPEND=-XX:+UseCompressedClassPointers -XX:+UseCompressedOops -Xshare:on -XX:SharedArchiveFile=/deployments/quarkus-app/app-cds.jsa -Dquarkus.http.host=0.0.0.0
AB_JOLOKIA_OFF=true
JAVA_APP_JAR=quarkus-run.jar
JAVA_APP_DIR=/deployments/quarkus-app
